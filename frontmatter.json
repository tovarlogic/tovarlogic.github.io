{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "isbn",
          "name": "isbn",
          "type": "number"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "datetime"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "excerpt",
          "name": "excerpt",
          "type": "string"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.content.pageFolders": [
    {
      "title": "_posts",
      "path": "[[workspace]]/_posts"
    },
    {
      "title": "_recetas",
      "path": "[[workspace]]/_recetas"
    },
    {
      "title": "_docs",
      "path": "[[workspace]]/_docs"
    }
  ],
  "frontMatter.templates.enabled": false,
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.preview.host": "http://localhost:4000"
}